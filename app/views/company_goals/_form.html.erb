<%= simple_form_for(@company_goal) do |f| %>
  <%= f.error_notification %>

  <div class="form-material">
    <%= f.input :objective,  label: false, placeholder: "Objective" %>
    <%= f.input :key_results, label: false, placeholder: "Key Results" %>
    <%= f.input :deadline %>
    <%= f.input :complete %>
  </div>

  <div class="form-actions">
    <div class="new-btn">
      <%= f.button :submit, :class => 'waves-effect btn-large' %>
    </div>
  </div>
<% end %>
